<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="testentry_view" model="ir.ui.view">
            <field name="name">leih.tests</field>
            <field name="model">leih.testentry</field>
            <field name="arch" type="xml">
            <form string="Test Entry">
                <sheet>

                    <group>

                        <field name="name"/>
                        <field name="group" on_change="onchange_group(group, context)"/>
                        <field name="department"/>
                        <field name="rate"/>
                        <field name="refarence_value"/>
                        <field name="required_time"/>

                    </group>
                    <notebook>
                        <page string="Mufti">
                            <field name="entrr_parameters">
                                <form string="_____0i0i3eij23i">
                                    <group>
                                        <field name="name" class="oe_inline"/>
                                        <field name="reference_value" class="oe_inline"/>
                                        <field name="others"/>

                                    </group>

                                </form>

                            </field>


                        </page>

                    </notebook>

                </sheet>
            </form>

            </field>

        </record>




        <record id="testentry_action" model="ir.actions.act_window">
            <field name="name">testentry.form</field>
            <field name="res_model">leih.testentry</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
        </record>

        <!-- expense type tree -->
        <record model="ir.ui.view" id="testentry_tree" >
            <field name="name">leih</field>
            <field name="model">leih.testentry</field>
            <field name="arch" type="xml">
            <tree string="Testentry List">
                        <field name="name"/>
                        <field name="group"/>
                        <field name="department"/>
                        <field name="rate"/>
                        <field name="refarence_value"/>
                        <field name="required_time"/>



            </tree>
            </field>
        </record>


<!--        <menuitem name="Doctors" id="leih_doctors_menu" parent="leih_menu"/>-->
        <menuitem name="Test Entry" id="testentry_menu_mainform" parent="leih_group_menu" action="testentry_action"/>


    </data>
</openerp>