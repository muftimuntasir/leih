<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="group_view" model="ir.ui.view">
            <field name="name">leih.group</field>
            <field name="model">leih.group</field>
            <field name="arch" type="xml">
            <form string="Group">
                <sheet>

                    <group>

                        <field name="name"/>


                    </group>

                </sheet>
            </form>

            </field>

        </record>




        <record id="group_action" model="ir.actions.act_window">
            <field name="name">group.form</field>
            <field name="res_model">leih.group</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
        </record>

        <!-- expense type tree -->
        <record model="ir.ui.view" id="group_tree" >
            <field name="name">group tree</field>
            <field name="model">leih.group</field>
            <field name="arch" type="xml">
            <tree string="Alu List">
                        <field name="name"/>


            </tree>
            </field>
        </record>


<!--        <menuitem name="Doctors" id="leih_doctors_menu" parent="leih_menu"/>-->
        <menuitem name="Group" id="group_menu_mainform" parent="leih_group_menu" action="group_action"/>


    </data>
</openerp>