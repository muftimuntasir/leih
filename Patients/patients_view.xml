<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="patients_view" model="ir.ui.view">
            <field name="name">leih.views</field>
            <field name="model">leih.patients</field>
            <field name="arch" type="xml">
            <form string="Patients">
                <sheet>

                    <group>
                        <field name="mobile"/>
                        <field name="name"/>
                        <field name="age"/>
                        <field name="address"/>
                        <field name="sex"/>

                    </group>

                </sheet>
            </form>

            </field>

        </record>




        <record id="patients_action" model="ir.actions.act_window">
            <field name="name">patients.form</field>
            <field name="res_model">leih.patients</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
        </record>

        <!-- expense type tree -->
        <record model="ir.ui.view" id="patients_tree" >
            <field name="name">leih</field>
            <field name="model">leih.patients</field>
            <field name="arch" type="xml">
            <tree string="Alu List">
                        <field name="mobile"/>
                        <field name="name"/>
                        <field name="age"/>
                        <field name="address"/>
                        <field name="sex"/>


            </tree>
            </field>
        </record>


<!--        <menuitem name="Patients" id="leih_patients_menu" parent="leih_menu"/>-->
        <menuitem name="Patients" id="patients_menu_mainform" parent="leih_group_menu" action="patients_action"/>


    </data>
</openerp>