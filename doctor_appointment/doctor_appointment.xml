<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="doctor_appointment_view" model="ir.ui.view">
            <field name="name">Doctor Appointment</field>
            <field name="model">doctor.appointment</field>
            <field name="arch" type="xml">
            <form string="Appointment">
                <sheet>

                    <group>
                        <field name="name"/>
                        <field name="mobile_no"/>
                        <field name="doctor_id"/>
                        <field name="appointment_date"/>
                    </group>

                </sheet>
            </form>

            </field>

        </record>


        <record id="doctor_appointment" model="ir.actions.act_window">
            <field name="name">Room</field>
            <field name="res_model">doctor.appointment</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
        </record>

        <!-- expense type tree -->
        <record model="ir.ui.view" id="room_tree" >
            <field name="name">diagnostic</field>
            <field name="model">doctor.appointment</field>
            <field name="arch" type="xml">
            <tree string="Alu List">
                       <field name="name"/>
                        <field name="mobile_no"/>
                        <field name="doctor_id"/>
                        <field name="appointment_date"/>


            </tree>
            </field>
        </record>

         <menuitem name="Appoinment" id="appoinment_menu_mainform" parent="leih_group_menu" action="doctor_appointment"/>


    </data>
</openerp>